{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": "on",
      "other": "on",
      "strings": "on"
    }
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "debug.toolBarLocation": "docked",
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.useInlineViewWhenSpaceIsLimited": false,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.cursorBlinking": "expand",
  "editor.cursorWidth": 4,
  "editor.fontFamily": "'Monaspace Neon', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.fontWeight": "500",
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.formatOnType": false,
  "editor.guides.bracketPairs": "active",
  "editor.guides.indentation": false,
  "editor.inlineSuggest.enabled": true,
  "editor.letterSpacing": 0,
  "editor.lineHeight": 26,
  "editor.lineNumbers": "on",
  "editor.minimap.enabled": true,
  "editor.minimap.maxColumn": 240,
  "editor.quickSuggestionsDelay": 0,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "all",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollBeyondLastLine": false,
  "editor.smoothScrolling": true,
  "editor.snippetSuggestions": "bottom",
  "editor.suggestFontSize": 16,
  "editor.suggestLineHeight": 28,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
  "editor.wordWrap": "on",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.compactFolders": false,
  "files.eol": "\n",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true,
    "**/vendor/**": true
  },
  "git.confirmSync": false,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false
  },
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.currentLine.scrollable": false,
  "javascript.autoClosingTags": false,
  "javascript.preferences.importModuleSpecifier": "relative",
  "javascript.preferences.quoteStyle": "double",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jest.autoRevealOutput": "off",
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": false,
  "prettier.tabWidth": 2,
  "prettier.useTabs": false,
  "search.exclude": {
    "**/bower_components": true,
    "**/node_modules": true,
    "**/public/[abcdefghjklmnopqrstuvwxyz]*": true,
    "**/public/i[abcdefghijklmopqrstuvwxyz]*": true,
    "**/vendor/[abcdefghijkmnopqrstuvwxyz]*": true,
    "**/vendor/l[bcdefghijklmnopqrstuvwxyz]*": true,
    "storage/framework/views": true
  },
  "search.quickOpen.includeSymbols": true,
  "search.showLineNumbers": true,
  "svelte.enable-ts-plugin": true,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontWeight": "500",
  "terminal.integrated.fontWeightBold": "300",
  "terminal.integrated.letterSpacing": 1.2,
  "theme-by-language.themes": {
    "*": "Vim Dark Hard"
  },
  "typescript.format.insertSpaceAfterTypeAssertion": true,
  "update.showReleaseNotes": false,
  "vs-code-prettier-eslint.prettierLast": true,
  "window.commandCenter": false,
  "window.title": "${activeEditorMedium}${separator}${rootName} ${dirty}",
  "window.titleBarStyle": "native",
  "workbench.editor.enablePreview": false,
  "workbench.editor.showTabs": "single",
  "workbench.fontAliasing": "antialiased",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.tree.indent": 30,
  "zenMode.fullScreen": false,
  "tabnine.experimentalAutoImports": true,
  "terminal.integrated.fontFamily": "FiraCode Nerd Font",
  "typescript.preferences.preferTypeOnlyAutoImports": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.userWords": ["Firestore", "gifs", "Newtonsoft", "supabase"],
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.external.linuxExec": "integrated",
  "terminal.external.osxExec": "warp.app",
  "git.allowForcePush": true,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "prisma.showPrismaDataPlatformNotification": false,
  "git.openRepositoryInParentFolders": "never",
  "liveServer.settings.donotShowInfoMsg": true,
  "cursor.cpp.disabledLanguages": ["plaintext", "scminput", "json"],
  "editor.semanticTokenColorCustomizations": {
    "[Default Dark+]": {
      "rules": {
        "property.mandatory:xml": {
          "bold": true
        },
        "string.i18n:xml": {
          "italic": true
        }
      }
    },
    "[Default Light+]": {
      "rules": {
        "property.mandatory:xml": {
          "bold": true
        },
        "string.i18n:xml": {
          "italic": true
        }
      }
    },
    "[Default High Contrast]": {
      "rules": {
        "property.mandatory:xml": {
          "bold": true
        },
        "string.i18n:xml": {
          "italic": true
        }
      }
    }
  },
  "git.autofetch": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
}
